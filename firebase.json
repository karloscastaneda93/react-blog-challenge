{
	"hosting": {
		"public": "build",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		],
		"headers": [
			{
				"source": "**/index.html",
				"headers": [
					{
						"key": "X-Robots-Tag",
						"value": "index"
					}
				]
			}
		]
	}
}
